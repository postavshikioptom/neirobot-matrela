{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "env": {
        "DEFAULT_MINIMUM_TOKENS": ""
      }
    },
    "sequentialthinking": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "alwaysAllow": [
        "sequentialthinking"
      ]
    },
    "tavily": {
      "command": "npx",
      "args": [
        "-y",
        "tavily-mcp@0.2.3"
      ],
      "env": {
        "TAVILY_API_KEY": "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"
      },
      "alwaysAllow": [
        "tavily-search",
        "tavily_search"
      ]
    },
    "neirobot-trade-mat Docs": {
      "url": "https://gitmcp.io/postavshikioptom/neirobot-trade-mat",
      "disabled": false,
      "autoApprove": []
    },
	"deepwiki mcp": {
      "env": {},
      "args": [
        "/c",
        "npx",
        "-y",
        "mcp-deepwiki@latest"
      ],
      "command": "cmd"
    }
  }
}